{
  "name": "broken-logistics-frontend",
  "version": "1.0.0",
  "description": "Frontend React application for BrokenLogistics - Intentionally vulnerable logistics platform",
  "private": true,
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "vulnerability:demo": "npm run dev"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "axios": "^1.2.2",
    "tailwindcss": "^3.2.4",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "framer-motion": "^8.0.2",
    "react-hook-form": "^7.41.5",
    "react-query": "^3.39.2",
    "zustand": "^4.1.5",
    "date-fns": "^2.29.3",
    "recharts": "^2.3.2",
    "react-hot-toast": "^2.4.0",
    "react-qr-code": "^2.0.11",
    "qr-scanner": "^1.4.2",
    "leaflet": "^1.9.3",
    "react-leaflet": "^4.2.0",
    "dompurify": "^2.4.1",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "classnames": "^2.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/leaflet": "^1.9.0",
    "@types/dompurify": "^2.4.0",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.191",
    "@types/jest": "^29.2.5",
    "@vitejs/plugin-react": "^3.0.0",
    "vite": "^4.0.0",
    "typescript": "^4.9.4",
    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "cypress": "^12.3.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "logistics",
    "security",
    "vulnerable",
    "education",
    "frontend"
  ],
  "author": "BrokenLogistics Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/broken-logistics.git",
    "directory": "frontend"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/src/setupTests.ts"],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/main.tsx",
      "!src/vite-env.d.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "vulnerabilities": {
    "intentional": [
      "XSS via dangerouslySetInnerHTML usage",
      "Client-side data exposure",
      "Insecure local storage usage",
      "Missing CSP headers",
      "Weak authentication token handling",
      "IDOR via client-side routing",
      "Insufficient input validation",
      "Exposed API endpoints in source",
      "Clickjacking vulnerabilities",
      "DOM-based XSS opportunities"
    ],
    "packages": {
      "dompurify": "May be bypassed with specific payloads",
      "lodash": "Prototype pollution vulnerabilities",
      "moment": "Known security issues in date parsing"
    }
  }
} 